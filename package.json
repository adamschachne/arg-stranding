{
  "name": "deathstranding",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "11.0.0",
    "npm": "6.4.1"
  },
  "scripts": {
    "dev": "node scripts/start.js --no-cache --runInBand",
    "server": "nodemon --inspect -r dotenv/config --watch server server/server.js",
    "server:full": "node scripts/serverFull.js",
    "heroku-postbuild": "cross-env GENERATE_SOURCEMAP=false node scripts/build.js",
    "build": "cross-env GENERATE_SOURCEMAP=false node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "start": "cross-env NODE_ENV=production node server/server.js",
    "lint": "eslint src --ext=.js,.jsx",
    "backup": "node -r dotenv/config scripts/backup.js"
  },
  "dependencies": {
    "@babel/core": "7.4.3",
    "@hot-loader/react-dom": "^16.8.6",
    "@material-ui/core": "^4.0.0",
    "@material-ui/icons": "^4.0.0",
    "@material-ui/styles": "^4.0.0",
    "@svgr/webpack": "4.1.0",
    "@types/classnames": "^2.2.7",
    "@types/jest": "^24.0.12",
    "@types/node": "^12.0.0",
    "@types/react": "^16.8.16",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.3",
    "@types/vis": "^4.21.15",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "async": "^2.6.2",
    "axios": "^0.18.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.7.1",
    "babel-loader": "8.0.5",
    "babel-plugin-named-asset-import": "^0.3.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-plugin-transform-remove-debugger": "^6.9.4",
    "babel-preset-react-app": "^8.0.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "cheerio": "^1.0.0-rc.3",
    "classnames": "^2.2.6",
    "connect-mongodb-session": "^2.1.1",
    "copy-to-clipboard": "^3.2.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "dotenv": "^8.0.0",
    "dotenv-expand": "5.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-react-app": "^4.0.0",
    "eslint-loader": "2.1.2",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "express": "^4.16.4",
    "express-https-redirect": "^1.0.0",
    "express-session": "^1.16.1",
    "fastpriorityqueue": "^0.6.1",
    "file-loader": "^3.0.1",
    "fs-extra": "^7.0.1",
    "google-spreadsheet": "^2.0.7",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "http-proxy-middleware": "^0.19.1",
    "identity-obj-proxy": "3.0.0",
    "image-size": "^0.7.4",
    "is-wsl": "^2.0.0",
    "jest": "24.7.1",
    "jest-environment-jsdom-fourteen": "0.1.0",
