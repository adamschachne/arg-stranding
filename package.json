{
  "name": "deathstranding",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "11.0.0",
    "npm": "6.4.1"
  },
  "scripts": {
    "dev": "node scripts/start.js",
    "server": "nodemon --inspect -r dotenv/config --watch server server/server.js",
    "server:full": "node scripts/serverFull.js",
    "heroku-postbuild": "cross-env GENERATE_SOURCEMAP=false node scripts/build.js",
    "build": "cross-env GENERATE_SOURCEMAP=false node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "start": "cross-env NODE_ENV=production node server/server.js",
    "lint": "eslint --ext=.js,.jsx src",
    "backup": "node -r dotenv/config scripts/backup.js"
  },
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "@svgr/webpack": "2.4.1",
    "axios": "^0.18.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-named-asset-import": "^0.3.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-plugin-transform-remove-debugger": "^6.9.4",
    "babel-preset-react-app": "^8.0.0",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "^2.4.2",
    "cheerio": "^1.0.0-rc.3",
    "classnames": "^2.2.6",
    "connect-mongodb-session": "^2.1.1",
    "copy-to-clipboard": "^3.2.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "dotenv": "^8.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-react-app": "^4.0.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "express": "^4.16.4",
    "express-https-redirect": "^1.0.0",
    "express-session": "^1.16.1",
    "fastpriorityqueue": "^0.6.1",
    "file-loader": "2.0.0",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "fs-extra": "^7.0.1",
    "google-spreadsheet": "^2.0.7",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "http-proxy-middleware": "^0.19.1",
    "identity-obj-proxy": "3.0.0",
    "image-size": "^0.7.4",
    "jest": "^24.7.1",
    "jest-pnp-resolver": "^1.2.1",
    "jest-resolve": "^24.7.1",
    "keycode": "^2.2.0",
    "localforage": "^1.7.3",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "^0.6.0",
    "mongodb": "^3.2.3",
    "muri": "^1.3.0",
    "ngrok": "^3.1.1",
    "nodemon": "^1.18.11",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "^1.4.3",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "^6.6.0",
    "postcss-safe-parser": "4.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.6.3",
    "react-app-polyfill": "^0.1.3",
    "react-dev-utils": "^6.1.1",
    "react-dom": "^16.6.3",
    "react-hot-loader": "^4.3.12",
    "react-router-dom": "^4.3.1",
    "react-spinners": "^0.5.4",
    "react-virtualized": "^9.21.0",
    "resolve": "1.8.1",
    "sass-loader": "7.1.0",
    "shelljs": "^0.8.3",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.3",
    "url-loader": "^1.1.2",
    "uuid": "^3.3.2",
    "vis": "^4.21.0",
    "webpack": "4.19.1",
    "webpack-dev-server": "^3.3.1",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "^4.3.0",
    "written-number": "^0.9.1"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
